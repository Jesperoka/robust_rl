    <actuator>

        <!-- Car actuators -->
        <velocity ctrllimited="true" ctrlrange="-0.8 0.8" gear="1 0 0 0 0 0" kv="8"
            joint="car_slider_x" name="car_vel_x" />
        <velocity ctrllimited="true" ctrlrange="-0.8 0.8" gear="1 0 0 0 0 0" kv="8"
            joint="car_slider_y" name="car_vel_y" />
        <velocity ctrllimited="true" ctrlrange="-1.0 1.0" gear="1 0 0 0 0 0" kv="8"
