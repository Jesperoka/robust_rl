    <keyframe>
        <key name="q_start"
            qpos="0 0 0 0.0 0.4 0.0 -2.0 0.0 2.3 0.78539816 0 0 0 0 0 0 0 0 0"
            ctrl="0 0 0 0.0 0.4 0.0 0.0 0.0 0.0 0.78539816 0 0 0 0" />
    </keyframe>

